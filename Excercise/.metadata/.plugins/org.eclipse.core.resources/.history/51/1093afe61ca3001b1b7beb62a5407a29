package geekTrust;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.PriorityQueue;

public class ProcessLogs {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		List<String> logs = new ArrayList<String>();
		logs.add("9 7 50");
		logs.add("22 7 50");
		logs.add("33 7 50");
		logs.add("22 7 50");
		System.out.println(processLogs(logs, 3));
		
		logs.clear();
		logs.add("1 2 50");
		logs.add("1 7 50");
		logs.add("1 3 50");
		logs.add("2 2 50");
		System.out.println(processLogs(logs, 3));

	}

	public static List<String> processLogs(List<String> logs, int thresold) {
		List<String> ans = new ArrayList<>();

		if (logs != null && logs.size() > 0) {
			PriorityQueue<String> temp = new PriorityQueue<>();
			Map<String, Integer> mem = new HashMap<>();
			for (String log : logs) {
				String[] sub = log.split(" ");
				mem.put(sub[0], mem.getOrDefault(sub[0], 0) + 1);
				mem.put(sub[1], mem.getOrDefault(sub[0], 0) + 1);
				if (mem.get(sub[0]) >= thresold) {
					temp.add(sub[0]);
				}
				if (mem.get(sub[1]) >= thresold) {
					temp.add(sub[1]);
				}
			}

			while (!temp.isEmpty()) {
				ans.add(temp.poll());
			}
		}
		return ans;
	}

}
